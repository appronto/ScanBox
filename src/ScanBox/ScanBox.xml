<?xml version="1.0" encoding="utf-8" ?>
<widget id="ScanBox.widget.ScanBox" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>ScanBox</name>
    <description>The description of this widget.</description>

    <icon></icon>

    <properties>
        <property key="scanAttr" type="attribute">
            <caption>Input attribute</caption>
            <category>Appearance</category>
            <description>The string/integer attribute to fill the scanned barcode</description>
            <attributeTypes>
                <attributeType name="String" />
                <attributeType name="Integer" />
            </attributeTypes>
        </property>
        <!--<property key="messageString" type="string" required="true" defaultValue="">
            <caption>Message</caption>
            <category>Appearance</category>
            <description>The message visible within the widget.</description>
        </property>-->
        <property key="onChangeMF" type="microflow" required="false" defaultValue="">
            <caption>Onchange Microflow</caption>
            <category>Behavior</category>
            <description>Run onchange microflow when the user is done typing.</description>
            <returnType type="Boolean" />
        </property>
        <property key="onChangeNF" type="nanoflow" required="false">
            <caption>Onchange Nanoflow</caption>
            <category>Behavior</category>
            <description>Run onchange nanoflow when the user is done typing.</description>
            <returnType type="Boolean" />
        </property>
        <property key="timeout" type="integer" defaultValue="500">
            <caption>Time out</caption>
            <category>Behavior</category>
            <description>Amount of milliseconds to trigger the onchange microflow and/or nanoflow after users stops typing.</description>
        </property>
    </properties>
</widget>

